{
  "name": "ebyroid",
  "version": "0.1.0",
  "description": "Node native addon for VOICEROID+ and VOICEROID2",
  "author": "Kinas <kinahasss@gmail.com> (https://github.com/nanokina)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nanokina/ebyroid.git"
  },
  "keywords": [
    "VOICEROID",
    "VOICEROID+",
    "VOICEROID2",
    "Discord",
    "Dispeak",
    "TTS",
    "Text To Speech"
  ],
  "main": "index.js",
  "files": [
    "CMakeLists.txt",
    "lib",
    "src"
  ],
  "type": "commonjs",
  "engines": {
    "node": ">=13.8.0"
  },
  "os": [
    "win32"
  ],
  "arch": [
    "ia32"
  ],
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^6.0.0",
    "debug": "^4.1.1",
    "iconv-lite": "^0.5.1"
  },
  "scripts": {
    "install": "cmake-js rebuild",
    "test:run": "@powershell -Command $env:DEBUG='*';node ./test/test_run"
  },
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "13.8.0",
    "arch": "ia32"
  },
  "husky": {
